<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0" 
    xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
    <vrfs xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-infra-rsi-cfg">
        <vrf>
            <vrf-name>MGMT_VRF</vrf-name>
            <create/>
            <description>MANAGEMENT_VRF</description>
        </vrf>
        <vrf>
            <vrf-name>EXTERNAL_PEERING</vrf-name>
            <create/>
            <vpn-id>
                <vpn-oui>202</vpn-oui>
                <vpn-index>254</vpn-index>
            </vpn-id>
            <afs>
                <af>
                    <af-name>ipv4</af-name>
                    <saf-name>unicast</saf-name>
                    <topology-name>default</topology-name>
                    <create/>
                    <bgp xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-ipv4-bgp-cfg">
                        <import-route-targets>
                            <route-targets>
                                <route-target>
                                    <type>as</type>
                                    <as-or-four-byte-as>
                                        <as-xx>0</as-xx>
                                        <as>65000</as>
                                        <as-index>1</as-index>
                                        <stitching-rt>0</stitching-rt>
                                    </as-or-four-byte-as>
                                </route-target>
                            </route-targets>
                        </import-route-targets>
                        <export-route-targets>
                            <route-targets>
                                <route-target>
                                    <type>as</type>
                                    <as-or-four-byte-as>
                                        <as-xx>0</as-xx>
                                        <as>65000</as>
                                        <as-index>1</as-index>
                                        <stitching-rt>0</stitching-rt>
                                    </as-or-four-byte-as>
                                </route-target>
                            </route-targets>
                        </export-route-targets>
                    </bgp>
                </af>
            </afs>
        </vrf>
    </vrfs>
</data>